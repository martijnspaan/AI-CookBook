# AI Cookbook Test Environment Secrets Configuration Template
# Copy this file to secrets.config and update with your actual values

# CosmosDB Configuration
COSMOSDB_CONNECTION_STRING="AccountEndpoint=https://your-cosmosdb.documents.azure.com:443/;AccountKey=YOUR_COSMOSDB_ACCOUNT_KEY_HERE;"

# Azure Container Registry Configuration
AZURE_CONTAINER_REGISTRY="your-registry.azurecr.io"
AZURE_CONTAINER_REGISTRY_USERNAME="your-registry-username"

# Azure Resource Configuration
AZURE_RESOURCE_GROUP="your-resource-group"
AZURE_LOCATION="westeurope"

# Container Configuration
CONTAINER_GROUP_NAME="ai-cookbook-unified"
API_CONTAINER_NAME="ai-cookbook-api"
WEB_CONTAINER_NAME="ai-cookbook-web"
