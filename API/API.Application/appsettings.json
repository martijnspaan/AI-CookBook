    "WeekMenuContainerName": "WeekMenu",
    "CookbookContainerName": "Cookbooks",
    "GroceryListContainerName": "GroceryLists",
    "PartitionKeyPath": "/id",
    "Throughput": 400,
    "CreateIfNotExists": true
  }
}
