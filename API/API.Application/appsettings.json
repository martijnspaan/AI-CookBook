{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HttpsRedirection": {
    "Enabled": false
  },
  "CosmosDb": {
    "ConnectionString": "${COSMOSDB_CONNECTION_STRING}",
    "DatabaseName": "CookBookTest",
